            {
              "title": "Successful Task Rate",
              "xyChart": {
                "dataSets": [
                  {
                    "timeSeriesFilter": {
                      "filter": "metric.type=\"custom.googleapis.com/spinnaker/clouddriver/task/finished\" AND metric.label.spin_service=\"clouddriver\" AND metric.label.success=True",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                ],
                "constantLines": [
                  {}
                ],
                "options": {},
                "y1Axis": {},
                "xAxis": {}
              }
            },

